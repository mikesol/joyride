{
	"name": "joyride",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "spago build && vite --open",
		"start:force-4": "spago build && VITE_FORCE_4=true vite --open",
		"start:no-gui": "spago build && VITE_LIL_GUI=false vite --open",
		"build:dev": "npm run build-ps && rm -rf dist && mkdir dist && vite build",
		"build": "npm run build-ps && rm -rf dist && mkdir dist && VITE_LIL_GUI=false VITE_FORCE_4=false VITE_FIREBASE_BUILD=production vite build",
		"build:prod": "npm run build-ps:prod && rm -rf dist && mkdir dist && PROD_EXPERIMENTAL=true VITE_LIL_GUI=false VITE_FORCE_4=false VITE_FIREBASE_BUILD=production vite build",
		"build:prod-no-min": "npm run build-ps:prod && rm -rf dist && mkdir dist && DISABLE_MINIMIZATION=true PROD_EXPERIMENTAL=true VITE_LIL_GUI=false VITE_FORCE_4=false VITE_FIREBASE_BUILD=production vite build",
		"build-ps": "spago build",
		"build-ps:prod": "spago -x prod.dhall build",
		"test": "spago -x test.dhall test"
	},
	"keywords": [],
	"author": "Mike Solomon",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@capacitor/cli": "^3.6.0",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"dotenv": "^16.0.1",
		"favicons": "^6.2.2",
		"postcss": "^8.4.13",
		"postcss-loader": "^6.2.1",
		"postcss-preset-env": "^7.5.0",
		"purescript": "^0.15.2",
		"purs-backend-es": "^1.0.1",
		"purs-tidy": "^0.8.0",
		"sass": "^1.54.0",
		"sass-loader": "^13.0.2",
		"spago": "^0.20.8",
		"style-loader": "^3.3.1",
		"tailwind": "^4.0.0",
		"tailwindcss": "^3.0.24",
		"vite": "^3.1.0",
		"vite-plugin-inject-preload": "^1.1.0"
	},
	"dependencies": {
		"@capacitor/android": "^3.6.0",
		"@capacitor/core": "^3.6.0",
		"@capacitor/ios": "^3.6.0",
		"file-select-dialog": "^1.5.4",
		"filestack-js": "^3.25.0",
		"firebase": "^9.8.3",
		"google-one-tap": "^1.0.6",
		"lil-gui": "^0.16.1",
		"openplayerjs": "^2.11.0",
		"pubnub": "^5.0.1",
		"screenfull": "^6.0.1",
		"three": "^0.140.0",
		"tone": "^14.7.77",
		"tw-elements": "^1.0.0-alpha12",
		"unique-names-generator": "^4.7.1",
		"uuid": "^8.3.2",
		"wavesurfer.js": "^6.2.0"
	}
}
